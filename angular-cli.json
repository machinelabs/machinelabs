{
  "project": {
    "version": "1.0.0-beta.22-1",
    "name": "machinelabs-client"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "ml",
      "mobile": false,
      "styles": [
        "styles.scss"
      ],
      "scripts": [
        "../node_modules/ace-builds/src-noconflict/ace.js",
        "../node_modules/ace-builds/src-noconflict/theme-github.js",
        "../node_modules/ace-builds/src-noconflict/mode-python.js",
        "../node_modules/ace-builds/src-noconflict/mode-sh.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "personal": "environments/environment.personal.ts",
        "offline": "environments/environment.offline.ts",
        "prod": "environments/environment.prod.ts",
        "test": "environments/environment.test.ts"
      }
    }
  ],
  "addons": [],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
