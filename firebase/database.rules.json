{
  "rules": {
    "idx": {
      "invocation_rate_proof": {
        "$userId": {
          ".validate": "newData.hasChildren(['timestamp', 'key'])",
          "timestamp": {
            ".validate": "newData.isNumber() && newData.isNumber() && newData.val() == now && (data.val() == null || newData.val() > data.val() + 1000)"
          },
          "key": {
            ".validate": "newData.isString()"
          },
          "$other": {
            ".validate": "false"
          },
          ".read": "auth != null && auth.uid == $userId",
          ".write": "auth != null && auth.uid == $userId && newData.val() != null"
        }
      },
      "recent_labs": {
        ".read": "true",
        ".indexOn": [
          "updated_at"
        ]
      },
      "user_labs": {
        "$id": {
          ".read": "true"
        }
      },
      "lab_executions": {
        "$id": {
          ".read": "true"
        }
      },
      "lab_visible_executions": {
        "$id": {
          ".read": "true"
        }
      },
      "user_executions": {
        "$id": {
          ".read": "auth.uid == 'execution-server'"
        }
      },
      "user_visible_executions": {
        "$id": {
          ".read": "true"
        }
      },
      "user_visible_labs": {
        "$id": {
          ".read": "true"
        }
      }
    },
    ".read": "auth.uid == 'execution-server'",
    "labs": {
      "$id": {
        "common": {
          ".validate": "newData.hasChildren(['id', 'user_id', 'name', 'description', 'directory', 'hidden', 'created_at', 'modified_at'])",
          "id": {
            ".validate": "newData.isString()"
          },
          "user_id": {
            ".validate": "newData.isString()"
          },
          "name": {
            ".validate": "newData.isString()"
          },
          "description": {
            ".validate": "newData.isString()"
          },
          "has_cached_run": {
            ".validate": "newData.isBoolean()"
          },
          "cache_hash": {
            ".validate": "newData.isString()"
          },
          "tags": {
            "$key1": {
              ".validate": "newData.isString()"
            },
            ".validate": "newData.hasChildren()"
          },
          "directory": {
            ".validate": "newData.isString()"
          },
          "hidden": {
            ".validate": "newData.isBoolean()"
          },
          "created_at": {
            ".validate": "newData.isNumber()"
          },
          "modified_at": {
            ".validate": "newData.isNumber()"
          },
          "fork_of": {
            ".validate": "newData.isString()"
          },
          "$other": {
            ".validate": "false"
          },
          ".read": "true",
          ".write": "data.val() != null ? auth != null && auth.uid == data.child('user_id').val() && (auth != null && auth.uid == newData.child('user_id').val()) || auth.uid == 'execution-server' || auth.uid == 'cloud-fn-assign-server' : auth != null && auth.uid == newData.child('user_id').val()"
        }
      },
      ".indexOn": [
        "common/cache_hash"
      ]
    },
    "invocations": {
      "$id": {
        "common": {
          ".validate": "newData.hasChildren(['id', 'user_id', 'timestamp', 'type', 'data'])",
          "id": {
            ".validate": "newData.isString()"
          },
          "user_id": {
            ".validate": "newData.isString()"
          },
          "timestamp": {
            ".validate": "newData.isNumber()"
          },
          "type": {
            ".validate": "newData.isString()"
          },
          "data": {
            ".validate": "newData.hasChildren() && newData.hasChildren(['id', 'directory']) || newData.hasChildren() && newData.hasChildren(['execution_id'])",
            "id": {
              ".validate": "newData.isString()"
            },
            "directory": {
              ".validate": "newData.isString()"
            },
            "$other": {
              ".validate": "false"
            },
            "execution_id": {
              ".validate": "newData.isString()"
            }
          },
          "$other": {
            ".validate": "false"
          },
          ".read": "auth != null && auth.uid == data.child('user_id').val() || auth.uid == 'cloud-fn-assign-server'",
          ".write": "(data.val() != null ? auth != null && auth.uid == data.child('user_id').val() && (auth != null && auth.uid == newData.child('user_id').val()) : auth != null && auth.uid == newData.child('user_id').val()) && $id == newData.parent().parent().parent().child('/idx/invocation_rate_proof/' + auth.uid + '/key').val() || auth.uid == 'execution-server' || auth.uid == 'cloud-fn-assign-server'"
        },
        ".read": "auth.uid == 'cloud-fn-assign-server' || auth.uid == 'execution-server'",
        ".write": "auth.uid == 'cloud-fn-assign-server'"
      },
      ".indexOn": [
        "server/id",
        "server/ahlem/timestamp",
        "server/hainholz/timestamp",
        "server/stephanskirchen/timestamp",
        "server/rijnbuurt/timestamp"
      ]
    },
    "executions": {
      "$id": {
        "common": {
          ".validate": "newData.hasChildren(['id', 'cache_hash', 'started_at', 'server_id', 'server_info', 'hardware_type', 'user_id', 'lab', 'status'])",
          "id": {
            ".validate": "newData.isString() && auth.uid == 'execution-server'"
          },
          "cache_hash": {
            ".validate": "newData.isString() && auth.uid == 'execution-server'"
          },
          "started_at": {
            ".validate": "newData.isNumber() && auth.uid == 'execution-server'"
          },
          "finished_at": {
            ".validate": "newData.isNumber() && auth.uid == 'execution-server'"
          },
          "failed_at": {
            ".validate": "newData.isNumber() && auth.uid == 'execution-server'"
          },
          "stopped_at": {
            ".validate": "newData.isNumber() && auth.uid == 'execution-server'"
          },
          "server_id": {
            ".validate": "newData.isString() && auth.uid == 'execution-server'"
          },
          "server_info": {
            ".validate": "newData.isString() && auth.uid == 'execution-server'"
          },
          "hardware_type": {
            ".validate": "newData.isString() && auth.uid == 'execution-server'"
          },
          "user_id": {
            ".validate": "newData.isString() && auth.uid == 'execution-server'"
          },
          "lab": {
            ".validate": "newData.hasChildren(['id', 'directory'])",
            "id": {
              ".validate": "newData.isString()"
            },
            "directory": {
              ".validate": "newData.isString()"
            },
            "$other": {
              ".validate": "false"
            }
          },
          "status": {
            ".validate": "newData.isString() && auth.uid == 'execution-server'"
          },
          "hidden": {
            ".validate": "newData.isBoolean()"
          },
          "name": {
            ".validate": "newData.isString()"
          },
          "$other": {
            ".validate": "false"
          },
          ".read": "true",
          ".write": "(data.val() != null ? auth != null && auth.uid == data.child('user_id').val() && (auth != null && auth.uid == newData.child('user_id').val()) : auth != null && auth.uid == newData.child('user_id').val()) || auth.uid == 'execution-server'"
        },
        "outputs": {
          ".read": "true"
        },
        "messages": {
          ".read": "true",
          ".write": "auth.uid == 'execution-server'",
          ".indexOn": [
            "index",
            "virtual_index",
            "kind"
          ],
          "$mid": {
            ".validate": "newData.hasChildren(['id', 'index', 'virtual_index', 'timestamp', 'data', 'kind', 'terminal_mode'])",
            "id": {
              ".validate": "newData.isString()"
            },
            "index": {
              ".validate": "newData.isNumber()"
            },
            "virtual_index": {
              ".validate": "newData.isNumber()"
            },
            "timestamp": {
              ".validate": "newData.isNumber()"
            },
            "data": {
              ".validate": "true"
            },
            "kind": {
              ".validate": "newData.isString()"
            },
            "terminal_mode": {
              ".validate": "newData.isBoolean()"
            },
            "$other": {
              ".validate": "false"
            },
            ".read": "true",
            ".write": "auth.uid == 'execution-server'"
          }
        }
      },
      ".indexOn": [
        "common/cache_hash"
      ]
    },
    "users": {
      "$id": {
        "common": {
          ".validate": "newData.hasChildren(['id', 'isAnonymous', 'displayName', 'bio', 'photoUrl', 'email'])",
          "id": {
            ".validate": "newData.isString() && newData.val() == auth.uid"
          },
          "isAnonymous": {
            ".validate": "newData.isBoolean() && newData.val() == (auth.provider == 'anonymous')"
          },
          "displayName": {
            ".validate": "newData.isString()"
          },
          "bio": {
            ".validate": "newData.isString()"
          },
          "photoUrl": {
            ".validate": "newData.isString()"
          },
          "email": {
            ".validate": "newData.isString() && newData.val() == auth.email"
          },
          "$other": {
            ".validate": "false"
          },
          ".read": "true",
          ".write": "(data.val() != null ? auth != null && auth.uid == data.child('id').val() && (auth != null && auth.uid == newData.child('id').val()) : auth != null && auth.uid == newData.child('id').val()) && (auth != null && auth.uid == $id)"
        },
        "plan": {
          ".read": "true"
        }
      }
    },
    "servers": {
      ".read": "auth.uid == 'execution-server' || auth.uid == 'cloud-fn-assign-server'",
      "$id": {
        ".validate": "newData.hasChildren(['id', 'hardware_type', 'name', 'disabled'])",
        "id": {
          ".validate": "newData.isString()"
        },
        "hardware_type": {
          ".validate": "newData.isString()"
        },
        "name": {
          ".validate": "newData.isString()"
        },
        "disabled": {
          ".validate": "newData.isBoolean()"
        },
        "$other": {
          ".validate": "false"
        }
      }
    },
    "handshakes": {
      "$id": {
        "request": {
          ".validate": "newData.hasChildren(['id'])",
          "id": {
            ".validate": "newData.isString()"
          },
          "token": {
            ".validate": "newData.isString()"
          },
          "$other": {
            ".validate": "false"
          },
          ".read": "auth != null && auth.uid == $id",
          ".write": "auth != null && auth.uid == $id"
        },
        "commit": {
          ".validate": "newData.hasChildren(['id', 'user_id'])",
          "id": {
            ".validate": "newData.isString()"
          },
          "user_id": {
            ".validate": "newData.isString()"
          },
          "$other": {
            ".validate": "false"
          },
          ".read": "auth != null && auth.uid == $id",
          ".write": "data.val() == null && (auth != null && auth.uid == newData.child('user_id').val())"
        }
      }
    }
  }
}
