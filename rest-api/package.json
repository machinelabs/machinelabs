{
  "name": "@machinelabs/rest-api",
  "version": "0.16.0+utc.2017.Oct.09-7.58.44",
  "description": "REST API for MachineLabs",
  "main": "index.js",
  "author": "MachineLabs, Inc <hello@machinelabs.ai> (https://machinelabs.ai)",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/storage": "^1.3.1",
    "@types/express": "^4.0.37",
    "express": "^4.16.1"
  },
  "engines" : { "node" : ">=8.4.0 <8.5.0" },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!@machinelabs/core)/"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "scripts": {
    "build": "node ./build/build.js",
    "start": "node ./dist/index.js",
    "tslint": "tslint -c tslint.json src/**/*.ts",
    "tsfix": "tslint --fix -c tslint.json src/**/*.ts",
    "test": "npm run tslint && jest"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.34",
    "@types/jest": "^19.2.2",
    "@types/node": "^6.0.45",
    "@types/source-map": "^0.1.27",
    "jest": "^20.0.4",
    "ts-jest": "^20.0.10",
    "typescript": "2.4.0",
    "tslint": "^5.5.0"
  }
}
