{
  "name": "@machinelabs/models",
  "version": "0.17.1+utc.2017.Oct.12-9.27.51",
  "description": "Models and data structures that can be shared across various projects.",
  "main": "index.js",
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!@machinelabs/models)/"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "scripts": {
    "postinstall": "tsc",
    "test": "./node_modules/jest/bin/jest.js",
    "tslint": "tslint  -c ./tslint.json './**/*.ts' --exclude './node_modules/**/*.ts' --exclude './**/*.d.ts'",
    "tsfix": "tslint --fix -c ./tslint.json './**/*.ts' --exclude './node_modules/**/*.ts' --exclude './**/*.d.ts'",
    "ci": "npm run tslint && npm run test"
  },
  "dependencies": {
    "@reactivex/rxjs": "^5.4.2",
    "@types/node": "^8.0.19"
  },
  "devDependencies": {
    "tslint": "^5.6.0",
    "typescript": "^2.4.2"
  }
}

