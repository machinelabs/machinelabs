{
  "name": "@machinelabs/models",
  "version": "0.25.0+utc.2018.Jan.29-9.22.00",
  "description": "Models shared across the @machinelabs ecosystem",
  "license": "MIT",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!@machinelabs/models)/"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "scripts": {
    "test": "./node_modules/jest/bin/jest.js",
    "tslint": "tslint  -c ./tslint.json './src/**/*.ts'",
    "tsfix": "tslint --fix -c ./tslint.json './src/**/*.ts'",
    "ci": "npm run tslint && npm run test"
  },
  "dependencies": {
    "@types/node": "^8.0.19"
  },
  "devDependencies": {
    "tslint": "^5.6.0",
    "typescript": "^2.4.2"
  }
}
